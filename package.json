{
	"name": "@r_wohl/web-channel-message",
	"version": "1.2.0",
	"description": "A light weight type-safe library for communicating via the Channel Message Web API",
	"main": "dist/esm/index.mjs",
	"module": "dist/esm/index.mjs",
	"types": "dist/cjs/index.d.ts",
	"type": "module",
	"scripts": {
		"build": "rimraf dist && npm run build:esm && npm run build:cjs",
		"build:cjs": "webpack --config webpack.cjs.config.js",
		"build:esm": "webpack --config webpack.esm.config.js"
	},
	"keywords": [
		"channel-message",
		"channel",
		"web-api"
	],
	"author": "Rami Wohl",
	"license": "ISC",
	"devDependencies": {
		"rimraf": "^5.0.5",
		"ts-loader": "^9.5.1",
		"typescript": "^5.3.3",
		"webpack": "^5.89.0",
		"webpack-cli": "^5.1.4"
	}
}
