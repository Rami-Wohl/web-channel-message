{
	"name": "@r_wohl/web-channel-message",
	"version": "2.0.0",
	"description": "A light weight type-safe library for communicating via the Channel Message Web API",
	"main": "dist/cjs/main/index.js",
	"types": "dist/cjs/main/index.d.ts",
	"module": "dist/esm/main/index.mjs",
	"type": "module",
	"scripts": {
		"build:cjs": "tsc --build src/main/tsconfig.cjs.json",
		"build:esm": "tsc --build src/main/tsconfig.esm.json",
		"build": "rimraf dist && npm run build:cjs && npm run build:esm"
	},
	"keywords": [
		"worker",
		"shared-worker",
		"web-api"
	],
	"author": "Rami Wohl",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "https://github.com/Rami-Wohl/web-channel-message"
	},
	"files": [
		"/dist",
		"package.json",
		"README.md"
	],
	"devDependencies": {
		"rimraf": "^5.0.5",
		"typescript": "^5.3.3"
	}
}
